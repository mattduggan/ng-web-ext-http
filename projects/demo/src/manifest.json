{
  "name": "ng-web-ext-http-demo",
  "version": "1.0.0",
  "description": "Demonstration of ng-web-ext-http",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "runtime.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [ "https://mail.google.com/*" ],
      "js": [
        "runtime.js",
        "polyfills.js",
        "styles.js",
        "vendor.js",
        "main.js"
      ],
      "all_frames": true
    }
  ]
}
